<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="banditoth.Forms.RecurrenceToolkitSample.Views.MainPageView"
             xmlns:pages="clr-namespace:banditoth.Forms.RecurrenceToolkitSample.Views">
  <FlyoutPage.Flyout>
    <pages:MainPageViewFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MainPageViewDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>